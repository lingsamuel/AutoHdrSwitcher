{
  "pollIntervalSeconds": 2,
  "pollingEnabled": false,
  "minimizeToTray": true,
  "enableLogging": true,
  "autoRequestAdminForTrace": false,
  "monitorAllFullscreenProcesses": false,
  "switchAllDisplaysTogether": false,
  "mainSplitterDistance": null,
  "runtimeTopSplitterDistance": null,
  "runtimeBottomSplitterDistance": null,
  "fullscreenIgnoreMap": {
    "pathprefix:C:\\Windows\\": true,
    "name:TextInputHost": true,
    "name:dwm": true
  },
  "displayAutoModes": {},
  "processRules": [
    {
      "pattern": "eldenring",
      "exactMatch": false,
      "caseSensitive": false,
      "regexMode": false,
      "enabled": true
    },
    {
      "pattern": "GameLauncher.exe",
      "exactMatch": true,
      "caseSensitive": true,
      "regexMode": false,
      "enabled": true
    },
    {
      "pattern": "^(ffxiv|ffxiv_dx11)\\.exe$",
      "exactMatch": false,
      "caseSensitive": false,
      "regexMode": true,
      "enabled": true
    }
  ]
}
